{
  "name": "showcase-airbnb-geosearch",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "parcel index.html --port 3000",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "typesenseServer": "docker run -i -p 8108:8108 -v$(pwd)/typesense-server-data/:/data typesense/typesense:0.22.0.rcs30 --data-dir /data --api-key=xyz --listen-port 8108 --enable-cors",
    "transformData": "node scripts/transform_data.js",
    "indexData": "node scripts/index_data.js",
    "build": "parcel build index.html --public-url https://airbnb-geosearch.typesense.org"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.13.3",
    "@parcel/transformer-sass": "2.13.3",
    "@parcel/transformer-webmanifest": "2.13.3",
    "babel-eslint": "10.1.0",
    "buffer": "^5.5.0||^6.0.0",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "events": "^3.1.0",
    "parcel": "^2.13.3",
    "prettier": "3.4.2",
    "process": "^0.11.10",
    "sass": "^1.83.0",
    "svgo": "^3"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "cheerio": "^1.0.0-rc.10",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.4.7",
    "instantsearch.js": "^4.75.6",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "node-fetch": "^3.3.2",
    "typesense": "^1.8.2",
    "typesense-instantsearch-adapter": "^2.8.0"
  }
}
